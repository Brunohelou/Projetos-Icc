#include <stdio.h>
#include <stdlib.h>

int main()
{
int x, y, z;
    printf("\tDigite os tres Valores inteiros\n\n");
    scanf("%d %d %d", &x, &y, &z);
    if(x==y  && x==z)
    printf("Maior Valor: %d", x);
    if(x==y && x>z)
    printf("Maior Valor: %d", x);
    if(x==z && x>y)
    printf("Maior Valor: %d", x);
    if(y==z && y>x)
    printf("Maior Valor: %d", y);
    if(x>y && x>z)
    printf("Maior Valor: %d", x);
    if(y>x && y>z)
    printf("Maior Valor: %d", y);
    if(z>x && z>y)
    printf("Maior Valor: %d", z);

    return 0;
}
